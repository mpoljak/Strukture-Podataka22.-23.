#define _CRT_SECURE_NO_WARNINGS 
#define MAX_NAME (1024)
#define ERROR (-1)
#define EXIT_SUCCESS (0)

#include <stdio.h>
#include <stdlib.h>
#include  <string.h>

struct _person;
typedef struct _person *Position;
typedef struct _person {
	char firstName[MAX_NAME];
	char lastName[MAX_NAME];
	int birthYear;
	Position next;
} Person;

int main(int argc, char** argv) {
	Person head = {
		.firstName = {0},
		.lastName = {0},
		.birthYear = 0,
		.next = NULL

	};
	return EXIT_SUCCESS;
}
